{
  "title": "Business Strategy & AI Vision",
  "pages": [
    {
      "name": "page1",
      "title": "AI Investment Priorities",
      "elements": [
        {
          "type": "panel",
          "name": "aiInvestmentPrioritiesPanel",
          "title": "Q17. AI Investment Priorities (Allocate 100% across categories)",
          "elements": [
            {
              "type": "text",
              "name": "techInfrastructurePct",
              "title": "Technology/Infrastructure (%)",
              "inputType": "number",
              "min": 0,
              "max": 100,
              "step": 1
            },
            {
              "type": "text",
              "name": "talentPct",
              "title": "Talent (hiring, training) (%)",
              "inputType": "number",
              "min": 0,
              "max": 100,
              "step": 1
            },
            {
              "type": "text",
              "name": "dataImprovementPct",
              "title": "Data improvement (%)",
              "inputType": "number",
              "min": 0,
              "max": 100,
              "step": 1
            },
            {
              "type": "text",
              "name": "processReengineeringPct",
              "title": "Process reengineering (%)",
              "inputType": "number",
              "min": 0,
              "max": 100,
              "step": 1
            },
            {
              "type": "text",
              "name": "changeManagementPct",
              "title": "Change management (%)",
              "inputType": "number",
              "min": 0,
              "max": 100,
              "step": 1
            },
            {
              "type": "text",
              "name": "externalPartnershipsPct",
              "title": "External partnerships (%)",
              "inputType": "number",
              "min": 0,
              "max": 100,
              "step": 1
            },
            {
              "type": "expression",
              "name": "totalAllocationCalculated",
              "title": "Total allocated: {totalAllocationCalculated}%",
              "expression": "iif({techInfrastructurePct} notempty, {techInfrastructurePct}, 0) + iif({talentPct} notempty, {talentPct}, 0) + iif({dataImprovementPct} notempty, {dataImprovementPct}, 0) + iif({processReengineeringPct} notempty, {processReengineeringPct}, 0) + iif({changeManagementPct} notempty, {changeManagementPct}, 0) + iif({externalPartnershipsPct} notempty, {externalPartnershipsPct}, 0)",
              "displayStyle": "decimal"
            }
          ],
          "validators": [
            {
              "type": "expression",
              "expression": "{techInfrastructurePct} + {talentPct} + {dataImprovementPct} + {processReengineeringPct} + {changeManagementPct} + {externalPartnershipsPct} == 100",
              "text": "The total allocation must equal 100%"
            }
          ]
        }
      ]
    }
  ]
}
